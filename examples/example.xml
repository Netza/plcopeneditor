              </type>
            </variable>
            <variable name="Control_State">
              <type>
                <BOOL/>
              </type>
            </variable>
          </outputVars>
        </interface>
        <body>
          <ST>
<![CDATA[
IF Collision THEN
  Speed := 0;
  Brakes := TRUE;
END_IF;

IF (Gate = open) AND
    (Pump = TRUE) AND (Temp > 200.0) THEN
  Control_State := TRUE;
ELSE
  Control_State := FALSE;

  PumpSpeed := 10.0;
END_IF;
]]>
          </ST>
