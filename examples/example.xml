      <pou name="ILTest" pouType="function">
        <interface>
          <returnType>
            <REAL/>
          </returnType>
          <inputVars>
            <variable name="X1">
              <type>
                <REAL/>
              </type>
            </variable>
          </inputVars>
          <localVars>
            <variable name="Temp">
              <type>
                <REAL/>
              </type>
            </variable>
          </localVars>
          <inputVars>
            <variable name="X2">
              <type>
                <REAL/>
              </type>
            </variable>
            <variable name="Y1">
              <type>
                <REAL/>
              </type>
            </variable>
            <variable name="Y2">
              <type>
                <REAL/>
              </type>
            </variable>
            <variable name="TMax">
              <type>
                <REAL/>
              </type>
            </variable>
          </inputVars>
          <outputVars>
            <variable name="ERROR">
              <type>
                <BOOL/>
              </type>
            </variable>
          </outputVars>
        </interface>
        <body>
          <IL>
<![CDATA[
     LD   Y1
     SUB  Y2     (* Substract Y2 from Y1 *)
     ST   Temp   (* Store Y1-Y2 in Temp *)
     MUL  Temp   (* Multiply by Temp to square *)
     ADD( X1     (* Defer ADD *)
     SUB  X2     (* Substract X1 from X2 *)
     ST   Temp   (* Store X1-X2 in Temp *)
     MUL  Temp   (* Multiply by Temp to square *)
     )
     SQRT        (* Call Square root fun *)
     ST   ILTest (* Setup function result *)
     GT   TMax   (* Greater than TMax ? *)
     JMPC ERR    (* Yes, Jump to Error *)
     S    ERROR  (* Set ERROR *)
     RET         (* Normal return *)
ERR: RET         (* Error return, ENO not set *)
]]>
          </IL>
        </body>
      </pou>
